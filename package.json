{
	"name": "multiverse-vis",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://abhsarma.github.io/milliways/",
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"start": "sirv public --no-clear --single --port 3000",
		"deploy": "gh-pages -d public", 
		"compile": "npm run build && (npm run start & sleep 3) && (inliner http://localhost:3000/index.html > template.html) && killall -9 node"
	},
	"exports": {
		".": "./src/main.js"
	},
	"type": "module",
	"devDependencies": {
		"@rollup/plugin-commonjs": ">=28.0.1",
		"@rollup/plugin-image": ">=3.0.3",
		"@rollup/plugin-json": ">=6.1.0",
		"@rollup/plugin-node-resolve": ">=15.3.0",
		"@rollup/plugin-terser": ">=0.4.4",
		"css-loader": ">=7.1.2",
		"csv-loader": ">=3.0.5",
		"jest": ">=29.7.0",
		"rollup": ">=4.24.3",
		"rollup-plugin-css-only": ">=4.5.2",
		"rollup-plugin-inject-process-env": ">=1.3.1",
		"rollup-plugin-livereload": ">=2.0.5",
		"rollup-plugin-svelte": ">=7.2.2",
		"style-loader": ">=4.0.0",
		"svelte": ">=5.1.9",
		"svelte2tsx": ">=0.7.22",
		"xml-loader": ">=1.2.1"
	},
	"dependencies": {
		"@emotion/css": ">=11.13.4",
		"bootstrap-grid": ">=3.0.1",
		"d3-array": ">=3.2.4",
		"d3-brush": ">=3.0.0",
		"d3-drag": ">=3.0.0",
		"d3-scale": ">=4.0.2",
		"d3-selection": ">=3.0.0",
		"d3-shape": ">=3.2.0",
		"gh-pages": "^6.2.0",
		"inliner": ">=1.13.1",
		"lodash": ">=4.17.21",
		"shelljs": "^0.8.5",
		"sirv-cli": ">=3.0.0"
	}
}
